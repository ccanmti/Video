<h1>動画一覧</h1>
<div class = "container">
  <div class = "row">
    <% @movies.each do |movie| %>
    <div class = "col-3">
    <%= video_tag(movie.video.to_s, loop: true, autoplay: false, muted: true, controls: true) %>
    <%= link_to movie.name, movie_path(movie.id) %>
    </div>
    <% end %>
  </div>
</div>